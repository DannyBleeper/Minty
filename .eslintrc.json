{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "no-console": "off",
        "indent": [
            "error",
            4
        ],
        "eol-last": [
            "error",
            "never"
        ],
        "no-empty": "off",
        "padded-blocks": "off",
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "no-underscore-dangle": "off",
        "spaced-comment": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "warn",
        "class-methods-use-this": "off"
    }
}